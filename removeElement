class Solution {
    public int removeElement(int[] nums, int val) {
        int len = nums.length;
        int i=0;
        int j=len-1;
        while(j>=i){
            if(nums[i]!=val){
                i++;
            }
            else if(nums[i]==val && nums[j]!=val){
                nums[i] = nums[j];
                i++;
                j--;
            }
            else if(nums[i]==val && nums[j]==val){
                j--;
            }
        }
        return j+1;
    }
}
